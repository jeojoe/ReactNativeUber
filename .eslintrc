{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "plugin": [
    "react"
  ],
  "extends": "airbnb",
  "rules": {
    "jsx-a11y/img-has-alt": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "no-use-before-define": 0,
    "import/no-extraneous-dependencies": 1,
    "react/prop-types": 0,
  }
}